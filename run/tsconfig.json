{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noImplicitAny": true,
    "skipLibCheck": true,
    "pretty": true,
    "removeComments": true,
    "noEmit": true,
    "types": [
      "node"
    ]
  },
  "exclude": [
    "../node_modules"
  ]
}
